<ul class="types-list">
  <div class="row">
        {{#each Types}}
        <div class="col-md-3 types-list-item">
            <div class="action-{{@key}} types-list-title">{{@key}}</div>
            <ul class="types-list">
                {{#each Types}}
                <li class="types-list-item"> {{this}}</li>
                {{/each}}
            </ul>
        </div>
        {{/each}}
  </div>
</ul>